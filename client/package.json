{
   "name": "fb-packages-commponents",
   "version": "0.1.0",
   "private": true,
   "description": "UI Facebook clone of facebook",
   "license": "MIT",
   "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "test": "jest",
      "test:watch": "jest --watch --env=jsdom",
      "storybook": "start-storybook -p 6006 --no-dll",
      "build-storybook": "build-storybook --no-dll",
      "lint": "npx eslint ./**/*",
      "lint:fix": "npx eslint --fix ./**/*",
      "format": "prettier --write . --config ./.prettierrc",
      "analize": "cross-env ANALIZE=true yarn build",
      "analyze:server": "cross-env BUNDLE_ANALYZE=server yarn build",
      "analyze:browser": "cross-env BUNDLE_ANALYZE=browser yarn build"
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged"
      }
   },
   "lint-staged": {
      "./**/*.{ts,tsx}": [
         "yarn run lint:fix"
      ]
   },
   "repository": {
      "url": "https://github.com/jhony-24/facebook-clone"
   },
   "author": {
      "url": "https://github.com/jhony-24",
      "name": "Jhony Vega"
   },
   "keywords": [
      "react",
      "facebook-clone",
      "next",
      "ui"
   ],
   "dependencies": {
      "@next/bundle-analyzer": "^10.0.4",
      "@types/react-custom-scrollbars": "^4.0.7",
      "@types/react-virtualized": "^9.21.10",
      "@types/styled-components": "^5.1.4",
      "axios": "^0.21.1",
      "framer-motion": "^2.9.4",
      "next": "10.0.0",
      "react": "17.0.1",
      "react-custom-scrollbars": "^4.2.1",
      "react-dom": "17.0.1",
      "react-hook-form": "^6.9.6",
      "react-icons": "^3.11.0",
      "react-virtualized": "^9.22.2",
      "recoil": "^0.1.2",
      "styled-components": "^5.2.0",
      "swr": "^0.3.8"
   },
   "devDependencies": {
      "@babel/core": "^7.12.3",
      "@babel/preset-env": "^7.12.7",
      "@babel/preset-react": "^7.12.7",
      "@storybook/addon-actions": "^6.0.28",
      "@storybook/addon-essentials": "^6.0.28",
      "@storybook/addon-links": "^6.0.28",
      "@storybook/addon-storysource": "^6.1.9",
      "@storybook/react": "^6.0.28",
      "@testing-library/jest-dom": "^5.11.6",
      "@testing-library/react": "^11.1.2",
      "@testing-library/react-hooks": "^3.4.2",
      "@testing-library/user-event": "^12.2.2",
      "@types/axios": "^0.14.0",
      "@types/faker": "^5.1.3",
      "@types/jest": "^26.0.15",
      "@types/node": "^14.14.5",
      "@types/react": "^16.9.54",
      "@types/react-dom": "^16.9.9",
      "@typescript-eslint/eslint-plugin": "^4.11.1",
      "@typescript-eslint/parser": "^4.11.1",
      "babel-loader": "^8.1.0",
      "babel-plugin-styled-components": "^1.11.1",
      "cross-env": "^7.0.3",
      "cypress": "^6.0.0",
      "eslint": "^7.14.0",
      "eslint-config-prettier": "^7.1.0",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-jsx-a11y": "^6.4.1",
      "eslint-plugin-prettier": "^3.3.0",
      "eslint-plugin-react": "^7.22.0",
      "eslint-plugin-react-hooks": "^4.2.0",
      "faker": "^5.1.0",
      "husky": "^4.3.6",
      "jest": "^26.6.3",
      "lint-staged": "^10.5.3",
      "prettier": "^2.2.1",
      "react-is": "^17.0.1",
      "ts-jest": "^26.4.4",
      "typescript": "^4.0.5"
   }
}
